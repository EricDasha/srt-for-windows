{
   "action": {
      "default_icon": {
         "128": "logo.png",
         "16": "logo.png",
         "48": "logo.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [
      {
         "all_frames": true,
         "js": [
            "subtitleParser.js",
            "subtitleEngine.js",
            "subtitleOverlay.js",
            "subtitleSettings.js",
            "content.js"
         ],
         "matches": [
            "<all_urls>"
         ],
         "run_at": "document_idle"
      }
   ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "logo.png",
      "16": "logo.png",
      "48": "logo.png"
   },
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "unlimitedStorage"
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "popup.html",
            "logo.png"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ],
   "key": "",
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "2.0.0"
}